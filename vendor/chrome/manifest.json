{
  "manifest_version": 2,
  "name": "HNProfile",
  "version": "1.0.0",
  "description": "HNProfile adds profile overlays to Hacker News usernames",
  "icons": {
  },
  "permissions": [
    "http://news.ycombinator.com/*",
    "https://news.ycombinator.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://news.ycombinator.com/*",
        "https://news.ycombinator.com/*"
      ],
      "css": [ "css/drop.css", "css/hnprofile.css" ],
      "js": [ "js/hnprofile.js" ]
    }
  ],
  "web_accessible_resources": [
    "img/*"
  ],
  "icons": {
    "16": "img/icon/16.png",
    "48": "img/icon/48.png",
    "128": "img/icon/128.png"
  }
}
